t.target;\n\n    if (checkbox === false) {\n      setTitle(value);\n      setImage(value);\n      setCommentSchema(value);\n    }\n  };\n\n  return (\n    \n    <div>\n      <h4><i class=\"fa-solid fa-cloud\"></i> Pictures Posted <i class=\"fa-regular fa-image\"></i></h4>\n\n      {Auth.loggedIn() ? (\n        <>\n            Picture Posted <i class=\"fa-solid fa-circle-check\"></i>: {false}\n            {pictures && pictures.map((picture) => \n          <form\n            className=\"flex-row justify-center justify-space-between-md align-center\"\n            onSubmit={handleFormSubmit}\n          >\n            <div className=\"col-12 col-lg-9\" data=\"slide-right\" key={picture._id}>\n              <input\n                checkbox={false}\n                value={picture.title}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n              <label forHtml={picture.title}>{picture.title}</label>\n            </div>\n            <div className=\"col-12 col-lg-9\" data=\"slide-right\" key={picture._id}>\n             <div id=\"inline_container\"></div>\n              <label forHtml={picture.image}>{picture.image}</label>\n            </div>\n            <div className=\"col-12 col-lg-9\" data=\"slide-left\">\n              <input\n                checkbox={false}\n                value={picture.comment}\n                className=\"form-input w-100\"\n                style={{ lineHeight: '1.5', resize: 'vertical' }}\n                onChange={handleChange}\n              ></input>\n               <label forHtml={picture.comment}>{picture.comment}</label>\n            </div>\n            <div className=\"col-12 col-lg-3\">\n              <button className=\"btn btn-primary btn-block py-3\" type=\"submit\">\n                Add Picture(s) <i class=\"fa-solid fa-file-import\"></i>\n              </button>\n            </div>\n          </form>\n            )}\n        </>\n      ) : (\n        <p>\n           ⛔ Log in to post picture(s)❗ Please❗{' '}\n          <Link to=\"/login\">Login <i id=\"login_icon\" class=\"fa-solid fa-users-rectangle\"></i></Link> or <Link to=\"/signup\">Sign Up <i class=\"fa-solid fa-user-plus\"></i></Link>\n        </p>\n      )}\n    </div>\n  );\n};\n\nexport default PostForm;","/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/components/Navbar.js",[],"/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/pages/Dashboard.js",[],"/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/components/PictureList.js",[],"/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/utils/mutations.js",[],"/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/utils/auth.js",["62"],"import decode from 'jwt-decode';\n\nclass AuthService {\n  getProfile() {\n    return decode(this.getToken());\n  }\n\n  loggedIn() {\n    const token = this.getToken();\n    // If there is a token and it's not expired, return `true`\n    return token && !this.isTokenExpired(token) ? true : false;\n  }\n\n  isTokenExpired(token) {\n    // Decode the token to get its expiration time that was set by the server\n    const decoded = decode(token);\n    // If the expiration time is less than the current time (in seconds), the token is expired and we return `true`\n    if (decoded.exp < Date.now() / 1000) {\n      localStorage.removeItem('id_token');\n      return true;\n    }\n    // If token hasn't passed its expiration time, return `false`\n    return false;\n  }\n\n  getToken() {\n    return localStorage.getItem('id_token');\n  }\n\n  login(idToken) {\n    localStorage.setItem('id_token', idToken);\n    window.location.assign('/');\n  }\n\n  logout() {\n    localStorage.removeItem('id_token');\n    window.location.reload();\n  }\n}\n\nexport default new AuthService();","/Users/AramsMac/Desktop/DESKTOP_REPO/raf-photo-cloud/client/src/utils/queries.js",[],{"rule